@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix tto: <http://example.org/tuto/ontology#> .
@prefix ttr: <http://example.org/tuto/resource#> .

# Resources - persons

ttr:John
	rdf:type dbo:Person ;
	dbp:name "John"^^xsd:string ;
	dbp:birthDate "1942-02-02"^^xsd:date ;
    dbo:parent ttr:William ;
	tto:sex "male"^^xsd:string ;
	tto:pet ttr:TomCat, ttr:LunaCat;
    .
    	
ttr:William
	rdf:type dbo:Person ;
	dbp:name "William"^^xsd:string ;
	dbp:birthDate "1978-07-20"^^xsd:date ;
	tto:sex "male"^^xsd:string ;
    dbo:parent ttr:Eve ;
    tto:pet ttr:RexDog;
	.

ttr:Eve
	rdf:type dbo:Person ;
	dbp:name "Eve"^^xsd:string ;
	dbp:birthDate "2006-11-03"^^xsd:date ;
	tto:sex "female"^^xsd:string ;
	#tto:pet ttr:FelixCat;
	.

# Resources - cats
ttr:TomCat
	rdf:type tto:Cat ;
	dbp:name "Tom"^^xsd:string ;
	tto:sex "male"^^xsd:string ;
	tto:color "grey"^^xsd:string ;
	tto:weight "5.8"^^xsd:decimal ;
	.

ttr:LunaCat
	rdf:type tto:Cat ;
	dbp:name "Luna"^^xsd:string ;
	tto:sex "female"^^xsd:string ;
	tto:color "violet"^^xsd:string ;
	tto:weight "4.2"^^xsd:decimal ;
    .

# Resources - dogs
ttr:RexDog
	rdf:type tto:Dog ;
	dbp:name "Rex"^^xsd:string ;
	tto:sex "male"^^xsd:string ;
	tto:color "brown"^^xsd:string ;
	tto:weight "8.8"^^xsd:decimal ;
	.

ttr:LonelyDog
	rdf:type tto:Dog ;
	dbp:name "Idefix"^^xsd:string ;
	tto:sex "male"^^xsd:string ;
	tto:color "white"^^xsd:string ;
	tto:weight "6.8"^^xsd:decimal ;
    .
