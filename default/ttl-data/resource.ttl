# Resources - persons

ttr:John
	rdf:type dbo:Person ;
	dbp:name "John"^^xsd:string ;
	dbp:birthDate "1982-02-02"^^xsd:date ;
	tto:sex "male"^^xsd:string ;
	tto:pet ttr:BuddyDog;
	.

ttr:William
	rdf:type dbo:Person ;
	dbp:name "William"^^xsd:string ;
	dbp:birthDate "1988-07-20"^^xsd:date ;
	tto:sex "male"^^xsd:string ;
	tto:friend ttr:John ;
	tto:pet ttr:TomCat;
	tto:pet ttr:LunaCat;
	.

ttr:Eve
	rdf:type dbo:Person ;
	dbp:name "Eve"^^xsd:string ;
	dbp:birthDate "1986-11-03"^^xsd:date ;
	tto:friend ttr:William ;
	tto:fanOf dbpedia:Harrison_Ford ;
	tto:sex "female"^^xsd:string ;
	#tto:pet ttr:FelixCat;
	.

dbpedia:Harrison_Ford
	tto:pet ttr:SnuffMonkey;
	.


# Resources - cats

ttr:TomCat
	rdf:type tto:Cat ;
	dbp:name "Tom"^^xsd:string ;
	tto:sex "male"^^xsd:string ;
	tto:color "black"^^xsd:string ;
	tto:weight "5.5"^^xsd:decimal ;
	.

ttr:LunaCat
	rdf:type tto:Cat ;
	dbp:name "Luna"^^xsd:string ;
	tto:sex "female"^^xsd:string ;
	tto:color "red"^^xsd:string ;
	.

ttr:FelixCat
	rdf:type tto:Cat ;
	dbp:name "Felix"^^xsd:string ;
	tto:sex "male"^^xsd:string ;
	tto:weight "7.2"^^xsd:decimal ;
	.

